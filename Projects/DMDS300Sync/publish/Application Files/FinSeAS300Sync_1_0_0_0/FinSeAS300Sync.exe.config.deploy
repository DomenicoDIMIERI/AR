            </setting>
            <setting name="UserMapping" serializeAs="String">
                <value>1:admin;204:Francesca.Lomanto;213:Vincenza.Novellino;208:Giovanna.Biscotti;216:Mina.Greco</value>
            </setting>
            <setting name="AutoSyncTime" serializeAs="String">
